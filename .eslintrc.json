{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ],
    "import/extensions": "off",
    "import/no-extraneous-dependencies": "off",
    "no-console": "off",
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "no-plusplus": 0,
    "react/forbid-prop-types": 0,
    "react/no-find-dom-node": 0,
    "no-param-reassign": 0
  }
}
